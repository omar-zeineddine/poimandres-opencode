    "diffHighlightAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffHighlightRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "selection",
      "light": "selection"
    },
    "diffRemovedBg": {
      "dark": "selection",
      "light": "selection"
    },
    "diffContextBg": {
      "dark": "background",
      "light": "background"
