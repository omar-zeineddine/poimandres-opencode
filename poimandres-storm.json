    "markdownImageText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCodeBlock": {
      "dark": "foreground",
      "light": "foreground"
    },
    "syntaxComment": {
      "dark": "brightBlack",
      "light": "brightBlack"
    },
    "syntaxKeyword": {
      "dark": "magenta",
      "light": "magenta"
    },
    "syntaxFunction": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxVariable": {
      "dark": "foreground",
      "light": "foreground"
    },
    "syntaxString": {
      "dark": "green",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "yellow",
      "light": "yellow"
    },
    "syntaxType": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxOperator": {
      "dark": "magenta",
      "light": "magenta"
    },
    "syntaxPunctuation": {
      "dark": "foreground",
      "light": "foreground"
    }
