      "dark": "foreground",
      "light": "foreground"
    },
    "markdownHeading": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownLinkText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCode": {
      "dark": "green",
      "light": "green"
    },
    "markdownBlockQuote": {
      "dark": "brightBlack",
      "light": "brightBlack"
    },
    "markdownEmph": {
      "dark": "magenta",
      "light": "magenta"
    },
    "markdownStrong": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownHorizontalRule": {
      "dark": "brightBlack",
      "light": "brightBlack"
    },
    "markdownListItem": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownImage": {
      "dark": "cyan",
      "light": "cyan"
